<h1>Books Page</h1>

<div class="row">
  <div class="col-md-5">
  <a href="/books/new" class="btn btn-success">New Book</a>
    {{#each books}}
    <div class="well">
      <h3>{{name}} {{author}}</h3>
      <div>$ {{price}}</div>
      <div>{{lastUpdated}}</div>
    </div>
    <hr/> {{/each}}
  </div>
</div>